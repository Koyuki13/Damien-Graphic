<div class="form-widget">
    {% for image in field.value %}
        <a href="#" class="ea-lightbox-thumbnail">
            <img class="image-preview" src="/uploads/images/{{ image.image }}" alt="{{ image.id }}">
        </a>

        <div id="ea-lightbox-{{ image.id }}" class="ea-lightbox">
            <img src="/uploads/images/{{ image.image }}" alt="{{ image.id }}">
        </div>

    {% endfor %}

</div>

{% block stylesheets %}
   {{ encore_entry_link_tags('image_preview') }}
{% endblock %}
